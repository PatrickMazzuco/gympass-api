{
  "name": "gympass-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsup",
    "start": "node dist/main/server.js",
    "start:dev": "tsx watch src/main/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --config vite.config.unit.ts",
    "test:unit:watch": "vitest --config vite.config.unit.ts",
    "test:unit:coverage": "vitest run --coverage --config vite.config.unit.ts",
    "test:unit:ui": "vitest --ui --config vite.config.unit.ts",
    "test:int": "vitest run --config vite.config.integration.ts",
    "test:int:watch": "vitest --config vite.config.integration.ts",
    "test:int:coverage": "vitest run --coverage --config vite.config.integration.ts",
    "test:int:ui": "vitest --ui --config vite.config.integration.ts",
    "migrate:dev": "dotenv -e env/.env.development prisma migrate dev",
    "migrate:test": "dotenv -e env/.env.test prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@vitest/coverage-c8": "0.30.1",
    "@vitest/ui": "0.30.1",
    "dotenv-cli": "7.2.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "pino-pretty": "10.0.0",
    "prettier": "2.8.7",
    "prisma": "4.13.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsup": "6.7.0",
    "tsx": "3.12.6",
    "typescript": "5.0.4",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.30.1"
  },
  "dependencies": {
    "@prisma/client": "4.13.0",
    "bcrypt": "5.1.0",
